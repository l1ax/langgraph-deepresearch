# LLM API Keys
DEEPSEEK_API_KEY=""
DEEPSEEK_BASE_URL="https://api.deepseek.com"

# Optional: OpenAI API Key (if using OpenAI models)
# OPENAI_API_KEY=""

# Optional: Anthropic API Key (if using Claude models)
# ANTHROPIC_API_KEY=""

# Search API
TAVILY_API_KEY=""

# Optional: LangSmith tracing
LANGSMITH_API_KEY=""
LANGSMITH_TRACING=true

# Optional: Zhipu AI
# ZHIPU_API_KEY=""

# ===== Checkpointer Configuration =====

# Environment: development | production
# - development: Uses MemorySaver (in-memory, no persistence, faster for local dev)
# - production: Uses PostgresSaver (persistent storage)
NODE_ENV=development

# Force PostgreSQL even in development (optional)
# By default, development uses MemorySaver
# Set USE_MEMORY_SAVER=false to use PostgreSQL in development
# USE_MEMORY_SAVER=false

# PostgreSQL Database URL
# Required when:
# - NODE_ENV=production, OR
# - USE_MEMORY_SAVER=false
# DATABASE_URL="postgresql://user:password@host:port/database"
